# Build Dockerfile
docker build -t meme{version}:1.0 .

# Create container
docker run -it -v $(pwd):/home/docker/meme --name meme{version} meme{version}:1.0

# Start container
docker start meme{version}

# Execute command on Docker container
docker exec -it meme5.3.3test2 sh -c "cd ./meme/apicalq0 && meme apical_q0.fasta -dna -oc . -nostatus -time 14400 -mod zoops -nmotifs 3 -minw 6 -maxw 50 -objfun classic -revcomp -markov_order 0" &
docker exec -it meme5.3.3test2 sh -c "cd ./meme/apicalq1 && meme apical_q1.fasta -dna -oc . -nostatus -time 14400 -mod zoops -nmotifs 3 -minw 6 -maxw 50 -objfun classic -revcomp -markov_order 0" &
